#!/usr/bin/env python3
import os
import sys


if __name__ == "__main__":
    if 'test' in sys.argv:
        os.environ.setdefault(
            "DJANGO_SETTINGS_MODULE", "emrapp.config.test_settings")
    else:
        os.environ.setdefault(
            "DJANGO_SETTINGS_MODULE", "emrapp.config.settings")

    from django.core.management import execute_from_command_line

    execute_from_command_line(sys.argv)
